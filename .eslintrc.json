{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "google"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "object-curly-spacing": "off",
        "linebreak-style": "off",
        "require-jsdoc": "off",
        "padded-blocks": "off",
        "max-len": "off",
        "indent": "off",
        "comma-dangle": "off",
        "arrow-parens": "off"
    }
}
